{
  "id": "nodejs-demo-webserver",
  "cpus": 0.5,
  "mem": 64.0,
  "disk": 10,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "registry.idc-chengdu.wanda.com:5000/nodejs-demo:prod",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8088, "hostPort": 0, "servicePort": 0, "protocol": "tcp" }
      ]
    }
  },
  "uris": [
    "file:///etc/docker.tar.gz"
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 120,
      "intervalSeconds": 120,
      "maxConsecutiveFailures": 2
    }
  ]
}
